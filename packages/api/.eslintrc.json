{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent":[
      "warn",
      2
    ],
    "no-unused-vars": "warn",
    "prettier/prettier":[
      "warn", {}, {
        "usePrettierrc": true
      }
    ]
  }
}